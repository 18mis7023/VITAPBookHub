                android:id="@+id/imgChangeYourLife"
                android:layout_width="140dp"
                android:layout_height="170dp"
                android:layout_marginTop="5dp"
                android:padding="10dp"
                android:background="@drawable/relative_layout_box"
                android:layout_marginLeft="5dp"
                android:src="@drawable/starts_with_food"/>
            <TextView
                android:id="@+id/txtChangeYourLife"
                android:layout_width="220dp"
                android:layout_height="65dp"
                android:layout_toRightOf="@+id/imgChangeYourLife"
                android:layout_marginTop="10dp"
                android:textSize="18sp"
                android:text="Change Your Life in unexpected ways"
                android:textColor="#000000"
                android:textStyle="bold"
                android:padding="5dp"
                android:textAlignment="center"
                />
            <TextView
                android:id="@+id/txtDallas"
                android:layout_width="220dp"
                android:layout_height="50dp"
                android:text="Dallas,Melissa HartWig"
                android:layout_below="@+id/txtChangeYourLife"
                android:layout_toRightOf="@+id/imgChangeYourLife"
                android:padding="10dp"
                android:textSize="20sp"
                android:textStyle="bold"
                android:textColor="#000000"
                android:textAlignment="center"/>
